# n8n Workflow Configuration
# Copy this file to .env and fill in your actual values

# n8n Webhook URL
# Get this from your n8n workflow "Webhook Trigger" node
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/phazur-video-webhook

# Google Cloud Project
GOOGLE_PROJECT_ID=your-project-id

# Phazur Labs Admin Portal
PHAZUR_ADMIN_URL=https://admin.phazurlabs.com
PHAZUR_API_TOKEN=your-api-token-here

# Google Drive Folder IDs
# Create folders in Google Drive and paste their IDs here
GDRIVE_AUDIO_FOLDER_ID=your-temp-audio-folder-id
GDRIVE_VIDEO_FOLDER_ID=your-course-videos-folder-id
GDRIVE_PHOTOS_FOLDER_ID=your-instructor-photos-folder-id

# Instructor Photo Google Drive File IDs
# Upload instructor photos and get their file IDs
INSTRUCTOR_PHOTO_SARAH_CHEN=your-sarah-chen-photo-file-id
INSTRUCTOR_PHOTO_MARCUS_WILLIAMS=your-marcus-williams-photo-file-id
INSTRUCTOR_PHOTO_ELENA_RODRIGUEZ=your-elena-rodriguez-photo-file-id
INSTRUCTOR_PHOTO_JAMES_PARK=your-james-park-photo-file-id
INSTRUCTOR_PHOTO_AISHA_KUMAR=your-aisha-kumar-photo-file-id
INSTRUCTOR_PHOTO_ALEX_THOMPSON=your-alex-thompson-photo-file-id

# Optional: n8n API (for programmatic workflow management)
N8N_API_KEY=your-n8n-api-key
N8N_BASE_URL=https://app.n8n.cloud/api/v1
